{
  "pipeline": {
    "levels": ["local", "hardware", "project"],
    "critical_section": true,
    "max_local_agents": 10,
    "io_delay_ms": 500,
    "timeout_minutes": 25,
    "start_delay_minutes": 5
  },
  "dpi": {
    "local": {
      "linear": 60,
      "log": 40
    },
    "family": {
      "linear": 70,
      "log": 45
    },
    "hardware": {
      "linear": 80,
      "log": 50
    },
    "project": {
      "linear": 100,
      "log": 60
    },
    "debug": 30
  },
  "axes": {
    "x_options": ["time", "count", "version"],
    "y_options": ["performance", "accuracy", "efficiency"],
    "show_error_bars": true,
    "accuracy_threshold": null,
    "version_rotation": 45
  },
  "io_optimization": {
    "compress_level": 1,
    "buffer_writes": true,
    "cleanup_old_hours": 2,
    "max_graph_per_batch": 5
  },
  "data_filters": {
    "min_performance": 0,
    "max_outlier_ratio": 10.0,
    "ignore_negative": true
  },
  "export": {
    "format": "json",
    "include_raw_data": false,
    "compress_export": false
  },
  "se_custom": {
    "comment": "SE’nin özelleştirmek istediği ayarları buraya ekleyin",
    "special_agents": {
      "PG1.2": {
        "dpi": 120,
        "priority": "high"
      }
    },
    "periodic_schedule": {
      "0-30min": ["project"],
      "30-60min": ["local", "project"],
      "60min+": ["local", "hardware", "project", "family"]
    }
  }
}
